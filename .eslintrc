{
  "extends": ["airbnb-standard"],
  "rules": {
    "max-len": "off",
    "consistent-return": "off",
    "no-continue": "off",
    "no-await-in-loop": "off",
    "no-param-reassign": "off",
    "no-restricted-syntax": "off",
    "guard-for-in": "off",
    "prefer-destructuring": "off",
    "no-bitwise": "off",
    "no-plusplus": "off",
    "padded-blocks": ["error", "always"]
  }
}
